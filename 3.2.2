/*
   Write an expression that will print "Dollar or more" 
   if the value of numCents is at least a dollar (100 cents is a dollar).
*/

import java.util.Scanner;

public class DetectDollar {
   public static void main (String [] args) {
      int numCents = 0;

      numCents = 109;

      if (/* Your solution goes here  */ numCents >= 100  /* End of solution */) {
         System.out.println("Dollar or more");
      }
      else {
         System.out.println("Not a dollar");
      }

      return;
   }
}
