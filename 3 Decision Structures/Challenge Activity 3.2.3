/*
   Write an expression that will print "Even" 
   if the value of userNum is an even number
*/

import java.util.Scanner;

public class DetectOdd {
   public static void main (String [] args) {
     int userNum = 0;

     userNum = 6;

      if (/* Your solution goes here  */ (userNum % 2) == 0  /* End of solution */) {
         System.out.println("Even");
      }
      else {
         System.out.println("Odd");
      }

      return;
   }
}
